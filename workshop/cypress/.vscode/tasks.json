{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cypress - ui",
            "command": "npx cypress open",
            "type": "shell",
            "group": "build"
        },
        {
            "label": "cypress - generate source",
            "command": "ecfeed",
            "args": [
                "--model", "0603-5525-0414-9188-9919",
                "--method", "com.example.test.Demo.typeString",
                "--template", "CSV",
                "--output", "cypress/fixtures/source.csv",
                "--nwise"
            ],
            "type": "shell",
            "group": "build"
        },
        {
            "label": "cypress - run @ csv",
            "command": "npx cypress run --spec cypress/e2e/csv.cy.js --browser firefox",
            "type": "shell",
            "group": "build"
        },
        {
            "label": "cypress - run/cloud @csv",
            "command": "npx cypress run --spec cypress/e2e/csv.cy.js --browser firefox --record --key be615cc3-1966-4364-afde-31a16798bca6",
            "type": "shell",
            "group": "build"
        }
    ]
}